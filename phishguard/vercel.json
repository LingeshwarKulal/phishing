{
  "version": 2,
  "builds": [
    {
      "src": "phishguard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/phishguard/dist/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/phishguard/dist/index.html"
    }
  ]
} 